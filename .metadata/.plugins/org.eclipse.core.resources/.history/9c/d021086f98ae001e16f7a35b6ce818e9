package d_Chapter03;

import java.util.ArrayList;

public class G_ArrayList02 {

	public static void main(String[] args) {
//		ArrayList 생성
		// : 문자열(String) 요소를 담는 배열
		ArrayList<String> Items = new ArrayList<>();
		
		Items.add("사과");
		Items.add("책");
		Items.add("연필");
		Items.add("시계");
		Items.add("컵");

	}

}
