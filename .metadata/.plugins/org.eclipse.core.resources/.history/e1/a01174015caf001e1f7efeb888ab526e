package e_Chapter04;

// 클래스 파일명 변경
// 파일 클릭 후 f2 버튼 클릭 or 파일 우클릭 Refactor에서 Rename
// : 파일 탐색기에서 임의로 변경하는 경우, 파일 내의 class명이 변경되지 않아 오류

// 클래스의 구조: 필드, 메소드, 생성자
class Car{
	// 클래스의 속성(필드)
	String model; // 모델
	int year; // 제조년도
	
	//클래스의 행동(메소드)
	
}
public class Object03 {

	public static void main(String[] args) {
		

	}

}
